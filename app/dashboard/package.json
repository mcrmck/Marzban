{
  "name": "marzban-dashboard",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "dev:admin": "vite --port 3000 --mode admin",
    "dev:portal": "vite --port 3001 --mode portal",
    "build": "tsc && vite build",
    "build:admin": "tsc && vite build --mode admin",
    "build:portal": "tsc && vite build --mode portal",
    "build:all": "npm run build:admin && npm run build:portal",
    "preview": "vite preview",
    "preview:admin": "vite preview --port 3000",
    "preview:portal": "vite preview --port 3001",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "gen:theme-typings": "npx chakra typegen ./src/theme/base.ts ./src/theme/adminTheme.ts ./src/theme/clientTheme.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.19.2",
    "@chakra-ui/toast": "^7.0.2",
    "@emotion/react": "^11.14.0",
    "@emotion/use-insertion-effect-with-fallbacks": "^1.2.0",
    "@emotion/utils": "^1.4.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@pandacss/is-valid-prop": "^0.53.7",
    "@tanstack/react-query": "^5.80.2",
    "@types/uuid": "^10.0.0",
    "apexcharts": "^4.7.0",
    "classnames": "^2.5.1",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "fast-safe-stringify": "^2.1.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "jsoneditor": "^10.2.0",
    "lodash.debounce": "^4.0.8",
    "next-themes": "^0.4.6",
    "ofetch": "^1.4.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "react-github-btn": "^1.4.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.2",
    "react-icons": "^5.5.0",
    "react-json-editor-ajrm": "^2.5.14",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.6.1",
    "react-slick": "^0.30.3",
    "react-use-websocket": "^4.13.0",
    "rollup-plugin-visualizer": "^6.0.1",
    "slick-carousel": "^1.8.1",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.49",
    "zustand": "^5.0.5",
    "zustand-computed": "^2.1.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.19.2",
    "@types/jsoneditor": "^9.9.5",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-json-editor-ajrm": "^2.5.6",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "sass": "^1.89.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
